<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
	<GenerateAssemblyVersionAttribute>false</GenerateAssemblyVersionAttribute>
	<Configurations>Debug;Release;Release-xml</Configurations>
	<AssemblyName>SampleBrowser.SfKanban</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="SamplesList\SamplesList.xml" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="SamplesList\SamplesList.xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SampleBrowser.Core" Version="28.1.33" />
    <PackageReference Include="Syncfusion.Xamarin.Core" Version="28.1.33" />
    <PackageReference Include="Syncfusion.Xamarin.DataSource" Version="28.1.33" />
    <PackageReference Include="Syncfusion.Xamarin.GridCommon" Version="28.1.33" />
    <PackageReference Include="Syncfusion.Xamarin.SfKanban" Version="28.1.33" />
    <PackageReference Include="Syncfusion.Xamarin.SfListView" Version="28.1.33" />
    <PackageReference Include="Xamarin.Forms" Version="5.0.0.2012" />
    <PackageReference Include="System.ComponentModel" Version="4.3.0" />
    <PackageReference Include="Syncfusion.Xamarin.SfRating" Version="28.1.33" />
  </ItemGroup>

  <ItemGroup>
    <Compile Condition=" '$(EnableDefaultCompileItems)' == 'true' " Update="App.xaml.cs">
      <DependentUpon>*.xaml</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Samples\KanbanCustomizationSample\KanbanCustomizationSample.xaml">
      <Generator>MSBuild:Compile</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\KanbanGettingStarted\KanbanGettingStarted.xaml">
      <Generator>MSBuild:Compile</Generator>
    </EmbeddedResource>
  </ItemGroup>
	
  <ItemGroup>
	<EmbeddedResource Update="Samples\KanbanCustomizationSample\Templates\MenuTemplate.xaml">
		<Generator>MSBuild:Compile</Generator>
	</EmbeddedResource>	
	<EmbeddedResource Update="Samples\KanbanCustomizationSample\Templates\OrderTemplate.xaml">
		<Generator>MSBuild:Compile</Generator>
	</EmbeddedResource>
	<EmbeddedResource Update="Samples\KanbanCustomizationSample\Templates\ReadyToServeTemplate.xaml">
		<Generator>MSBuild:Compile</Generator>
	</EmbeddedResource>
	<EmbeddedResource Update="Samples\KanbanCustomizationSample\Templates\DeliveryTemplate.xaml">
		<Generator>MSBuild:Compile</Generator>
	</EmbeddedResource>
	<EmbeddedResource Update="Samples\Themes\Themes.xaml">
	  <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
	</EmbeddedResource>
  </ItemGroup>
	
  <ItemGroup>
    <Folder Include="Samples\KanbanCustomizationSample\Templates\" />
  </ItemGroup>
</Project>