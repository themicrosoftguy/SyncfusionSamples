<?xml version="1.0" encoding="utf-8" ?>
<local:SampleView
             xmlns:local="clr-namespace:SampleBrowser.Core;assembly=SampleBrowser.Core"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SampleBrowser.Presentation.OLEObject">

  <local:SampleView.Content>
    <StackLayout Padding="10">
      <Label x:Name="Description" Text="This sample demonstrates how to insert and extract a OLE Object in PowerPoint presentation."
                 FontSize="Medium" VerticalOptions="Center"/>
      <Label></Label>
      <Button x:Name="btnInsertOle" WidthRequest="180" Clicked ="OnInsertOleButtonClicked" Text ="Create Presentation" HorizontalOptions="Center" VerticalOptions="Center"/>
      <Button x:Name="btnExtractOle" WidthRequest="180" Clicked ="OnExtractOleButtonClicked" Text ="Open Embedded File" HorizontalOptions="Center" VerticalOptions="Center"/>
    </StackLayout>
  </local:SampleView.Content>
</local:SampleView>
