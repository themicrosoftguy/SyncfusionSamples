<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <GenerateAssemblyVersionAttribute>false</GenerateAssemblyVersionAttribute>
    <Configurations>Debug;Release;Release-xml</Configurations>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="SamplesList\SamplesList.xml" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="SamplesList\SamplesList.xml" />
  </ItemGroup>

  <ItemGroup>
     <PackageReference Include="Syncfusion.Xamarin.Core" Version="28.1.33" /> 
    <PackageReference Include="SampleBrowser.Core" Version="28.1.33" />
    <PackageReference Include="Syncfusion.Xamarin.Core" Version="28.1.33" />
    <PackageReference Include="Syncfusion.Xamarin.DataSource" Version="28.1.33" />
    <PackageReference Include="Syncfusion.Xamarin.GridCommon" Version="28.1.33" />
    <PackageReference Include="Syncfusion.Xamarin.SfListView" Version="28.1.33" />
    <PackageReference Include="Syncfusion.Xamarin.SfSchedule" Version="28.1.33" />
    <PackageReference Include="Xamarin.Forms" Version="5.0.0.2012" />
  </ItemGroup>
  <ItemGroup>
    <Compile Condition=" '$(EnableDefaultCompileItems)' == 'true' " Update="App.xaml.cs">
      <DependentUpon>*.xaml</DependentUpon>
    </Compile>
  </ItemGroup> 
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DefineConstants>TRACE;DEBUG;NETSTANDARD2_0</DefineConstants>
    <NoWarn>1701;1702;1705;CS0436;MSB4011</NoWarn>
  </PropertyGroup>
    <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DefineConstants>TRACE;Release;NETSTANDARD2_0</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release-xml|AnyCPU' ">
    <DefineConstants>TRACE;Release-xml;NETSTANDARD2_0</DefineConstants>
  </PropertyGroup> 
   <Import Project="..\..\..\..\build\tools\Syncfusion.CodeAnalysis.StyleCop\tools\StyleCop.MsBuild.targets" Condition="Exists('..\..\..\..\build\tools\Syncfusion.CodeAnalysis.StyleCop\tools\StyleCop.MsBuild.targets')" />
</Project> 